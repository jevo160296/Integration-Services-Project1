          SQLTask:Connection="{9B0E6C70-C7C3-4828-8C2F-F6FF086129D3}"
          SQLTask:SqlStatementSource="DECLARE @FECHA_INICIAL CHAR(8)&#xA;DECLARE @FECHA_FINAL CHAR(8)&#xA;&#xA;SET @FECHA_INICIAL = '20060101'&#xA;SET @FECHA_FINAL   = '20221231';&#xA;SET LANGUAGE Spanish&#xA;&#xA;DELETE FROM [dim].[Fecha];&#xA;&#xA;--CREATE TABLE [dim].[Fecha](&#xA;--&#x9;[FechaKey] [int] NOT NULL,&#xA;--&#x9;[Fecha] [date] NOT NULL,&#xA;--&#x9;[Anio] [int] NOT NULL,&#xA;--&#x9;[Mes] [varchar](10) NOT NULL,&#xA;--&#x9;[Dia] [int] NOT NULL,&#xA;--&#x9;[Mes (Orden)] [int] NOT NULL,&#xA;--&#x9;[DiaSemana] [varchar](9) NOT NULL,&#xA;--&#x9;[DiaSemana(Orden)] [int] NOT NULL,&#xA;--&#x9;[Trimestre] [int] NOT NULL,&#xA;--&#x9;[Cuatrimestre] [int] NOT NULL,&#xA;--&#x9;[Semestre] [int] NOT NULL,&#xA;--&#x9;[EsDiaLaboral] [char](1) NOT NULL,&#xA;-- CONSTRAINT [PK_Fecha] PRIMARY KEY CLUSTERED &#xA;--(&#xA;--&#x9;[FechaKey] ASC&#xA;--)WITH (PAD_INDEX = OFF, STATISTICS_NORECOMPUTE = OFF, IGNORE_DUP_KEY = OFF, ALLOW_ROW_LOCKS = ON, ALLOW_PAGE_LOCKS = ON, OPTIMIZE_FOR_SEQUENTIAL_KEY = OFF) ON [PRIMARY]&#xA;--) ON [PRIMARY];&#xA;&#xA;WITH recursive_fecha&#xA;AS&#xA;(&#xA;SELECT CAST(@FECHA_INICIAL as datetime) AS [fecha]&#xA;UNION ALL&#xA;SELECT DATEADD(dd, 1, [fecha])&#xA;FROM recursive_fecha&#xA;WHERE DATEADD(dd, 1, [fecha]) &lt;= @FECHA_FINAL&#xA;),&#xA;null_fecha&#xA;AS&#xA;(&#xA;SELECT CAST('19000101' as datetime) AS [fecha]&#xA;UNION ALL&#xA;SELECT DATEADD(dd, 1, [fecha])&#xA;FROM recursive_fecha&#xA;WHERE DATEADD(dd, 1, [fecha]) &lt;= '19000101'&#xA;),&#xA;fecha_completo&#xA;AS&#xA;(&#xA;SELECT &#xA;&#x9;DATEPART(day,[fecha])+DATEPART(month,[fecha])*100 + DATEPART(year,[fecha])*10000 as FechaKey,&#xA;&#x9;[fecha] as Fecha, &#xA;&#x9;DATEPART(year, [fecha]) as Anio,&#xA;&#x9;DATENAME(month, [fecha]) as Mes,&#xA;&#x9;DATEPART(day, [fecha]) as Dia,&#xA;&#x9;DATEPART(month, [fecha]) as [Mes (Orden)],&#xA;&#x9;DATENAME(weekday, [fecha]) as DiaSemana,&#xA;&#x9;DATEPART(weekday, [fecha]) as [DiaSemana (Orden)],&#xA;&#x9;DATEPART(QUARTER, [fecha]) as Trimestre,&#xA;&#x9;CEILING(DATEPART(month,[fecha])*3.0/12.0) as Cuatrimestre,&#xA;&#x9;CEILING(DATEPART(quarter, [fecha])/2.0) as Semestre,&#xA;&#x9;CASE&#xA;&#x9;&#x9;WHEN CAST(DATEPART(WEEKDAY, [fecha]) as INT) &lt; 6 THEN '1'&#xA;&#x9;&#x9;ELSE '0'&#xA;&#x9;END&#xA;&#x9;as EsDiaLaboral&#xA;FROM &#xA;(SELECT *&#xA;FROM&#xA;recursive_fecha&#xA;UNION&#xA;SELECT *&#xA;FROM&#xA;null_fecha&#xA;) as calendario&#xA;)&#xA;&#xA;INSERT INTO dim.Fecha (FechaKey, Fecha, Anio, Mes, Dia, [Mes (Orden)], DiaSemana, [DiaSemana(Orden)], Trimestre, Cuatrimestre, Semestre, EsDiaLaboral)&#xA;SELECT * FROM fecha_completo&#xA;OPTION (MAXRECURSION 0)" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
